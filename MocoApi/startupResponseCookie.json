[
  {
    "description":"这是一个会返回cookie的get请求",
    "request":{
      "uri":"/returnCookie",
      "method":"get"

    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "cookies":{
        "login":"true"
      },
      "text":"获取返回的cookies"
    }
  },
  {
    "description":"这是一个带cookies信息的get请求",
    "request":{
      "uri":"/getCookies",
      "method":"get",
      "cookies":{
        "login":"true"
      }
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "text":"这是一个需要携带cookies信息才能访问的get请求"
    }
  },
  {
    "description":"这是一个带cookies信息的post请求",
    "request":{
      "uri":"/post/with/cookies",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "forms":{
        "name":"zhangsan"
      }
    },
    "response":{
      "headers":{
        "content-type":"text/html;charset=gbk"
      },
      "json":{
        "status":"200",
        "text":"这是带cookies信息的请求响应文本"
      }
    }
  },
  {
    "description":"这是一个返回cookies信息的post请求",
    "request":{
      "uri":"/post/returnCookies",
      "method":"post",
      "forms":{
        "name":"zhangsan"
      }
    },
    "response":{
      "headers":{
        "content-type":"text/html;charset=gbk"
      },
      "cookies":{
        "login":"true"
      },
      "json":{
        "status":"200",
        "text":"这是返回cookies信息的请求响应文本"
      }
    }
  }
]